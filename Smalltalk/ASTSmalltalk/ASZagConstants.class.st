{ #category : 'mapping' }
ASZagConstants class >> constantMap [

	^ Dictionary new
		  at: #pushLiteral put: ZfPushLiteral;
		  at: #returnTop put: ZfReturnTop;
		  yourself
]

{ #category : 'class initialization' }
ASZagConstants class >> initialize [

